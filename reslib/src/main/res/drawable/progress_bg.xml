<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--  设置背景色  -->
    <item
        android:id="@android:id/background"
        android:drawable="@drawable/bg_color_f6f8fa_corners_3dp_shape"></item>

    <!--  设置进度条颜色  -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3dp" />
                <gradient
                    android:endColor="@color/color_ff835e"
                    android:startColor="@color/color_ffbfa6" />
            </shape>
        </clip>
    </item>
</layer-list>