<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <solid android:color="#38C6C8C9" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/color_ffffff" />
            <stroke
                android:width="0.5dp"
                android:color="#38C6C8C9" />
            <corners android:radius="8dp" />
        </shape>
    </item>
</layer-list>