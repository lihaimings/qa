        <attr name="ringWidth" format="dimension" />
        <attr name="currentProgress" format="integer" />
        <attr name="maxProgress" format="integer" />
    </declare-styleable>

    <!--选择器-->
    <declare-styleable name="LoopView">
        <attr name="lv_textSize" format="dimension" />
        <attr name="lv_lineSpace" format="float" />
        <attr name="lv_centerTextColor" format="color" />
        <attr name="lv_outerTextColor" format="color" />
        <attr name="lv_dividerTextColor" format="color" />
        <attr name="lv_itemsVisibleCount" format="integer" />
        <attr name="lv_isLoop" format="boolean" />
        <attr name="lv_initialPosition" format="integer" />
        <attr name="lv_scaleX" format="float" />
    </declare-styleable>

    <!--圆形进度条-->
    <declare-styleable name="CircleProgressBar">
        <!--文字大小-->
        <attr name="circle_text_size" format="dimension" />
        <!--文字颜色-->
        <attr name="circle_text_color" format="color" />
        <!--文本-->
        <attr name="circle_text" format="string" />
        <!--变宽宽度-->
        <attr name="circle_stroke_width" format="dimension" />
        <!--未走的进度的颜色-->
        <attr name="circle_normal_color" format="color" />
        <!--已走的进度的颜色-->
        <attr name="circle_progress_color" format="color" />
        <!--已走的进度 0-360 -->
        <attr name="circle_progress" format="integer" />
        <!--类型 填充型，还是环形-->
        <attr name="circle_progress_style" format="enum">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>

</resources>